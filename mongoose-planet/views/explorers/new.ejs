<%- include('../partials/header')  %>
<link rel='stylesheet' href='/stylesheets/preformers/new.css' />
<%- include('../partials/nav')  %>
<main>
  <h1><%= title %></h1>
<p>Before adding, please ensure that the Explorer is not in the following list:
<select>
  <% explorers.forEach(function(explorer) { %>
    <option><%= explorer.name %> </option> 
  <% }) %>
</select>
</p>
<form id="add-explorer-form" action="/explorers" method="POST">
 
  <label for="name-input">Name: </label>
  <input type="text" name="name" id="name-input">

  <label for="age-input">Age: </label>
  <input type="number" name="age" id="name-input">

  <button type="submit"> Add Explorer</button>
</form>
</main>



<%- include('../partials/footer')  %>